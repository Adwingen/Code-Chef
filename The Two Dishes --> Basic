#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>
#include <stdlib.h>

int main() 
{
    int testCases;
    int maximumTastiness;
    int sumOfTastiness;
    int result;
    scanf("%d", &testCases);
    while (testCases--) 
    {
        scanf("%d %d", &maximumTastiness, &sumOfTastiness);
        result = sumOfTastiness > maximumTastiness ? sumOfTastiness - maximumTastiness : maximumTastiness - sumOfTastiness;
        result = maximumTastiness - result;

        printf("%d\n", result);

    }
	
	return 0;
}
