#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int main() 
{
	int testCases;
	int i;
	int lowNumber, highNumber, increment;
	int result;
	
	scanf("%d", &testCases);
	fgetc(stdin);
	for (i = 0; i < testCases; i++)
	{
		scanf("%d %d %d", &lowNumber, &highNumber,&increment);
		fgetc(stdin);
		result = (highNumber - lowNumber) / increment;
		printf("%d\n", result);
	}
	

	return 0;
}
